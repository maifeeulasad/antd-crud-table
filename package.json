{
  "name": "antd-crud-table",
  "private": false,
  "version": "0.0.14",
  "type": "module",
  "homepage": "https://github.com/maifeeulasad/antd-crud-table",
  "keywords": [
    "antd",
    "react",
    "crud",
    "table",
    "components"
  ],
  "license": "MIT",
  "author": "Maifee Ul Asad<maifeeulasad@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build:static": "tsc -b && vite build",
    "build:lib": "tsc --p ./tsconfig.build.json && vite build --config vite.config.lib.ts",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/pro-components": "^2.8.7",
    "antd": "^5.24.8",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.15.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^4.5.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "main": "./dist/CrudTable.cjs",
  "module": "./dist/CrudTable.js",
  "types": "./dist/CrudTable.d.ts",
  "exports": {
    ".": {
      "types": "./dist/CrudTable.d.ts",
      "import": "./dist/CrudTable.js",
      "require": "./dist/CrudTable.cjs"
    },
    "./CrudTableLazy": {
      "types": "./dist/CrudTableLazy.d.ts",
      "import": "./dist/CrudTableLazy.js",
      "require": "./dist/CrudTableLazy.cjs"
    }
  },
  "files": [
		"dist"
	]
}
